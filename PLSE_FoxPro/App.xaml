                <ResourceDictionary Source="pack://application:,,,/Dictionaries/Brushes_Colors.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/Button.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/TextBlock.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/Fonts.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/TextBox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/ComboBox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/Icon.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/CheckBox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/DatePicker.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/ListBoxItem.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/DataGrid.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/VariousControls.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Dictionaries/DataTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
