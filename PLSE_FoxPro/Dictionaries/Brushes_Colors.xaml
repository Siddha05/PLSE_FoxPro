<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="AcceptGreenBrush" Color="SpringGreen"/>
    <SolidColorBrush x:Key="RejectRedBrush" Color="Red"/>

    <SolidColorBrush x:Key="InactiveText" Color="Gray"/>

    <SolidColorBrush x:Key="HighlightedTextBlue" Color="DarkBlue"/>

    <SolidColorBrush x:Key="SelectedItemBrush" Color="#FF4E0789"/>
    <SolidColorBrush x:Key="MouseOverBrush" Color="#FFEBF5F9"/>

    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" x:Key="MessageDarkBrush">
        <GradientStop Color="#FF646464" Offset="0.2"/>
        <GradientStop Color="#FF585858" Offset="0.5"/>
        <GradientStop Color="#FF424242" Offset="1"/>
    </LinearGradientBrush>
</ResourceDictionary>